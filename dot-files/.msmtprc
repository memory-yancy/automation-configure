# User configuration for msmtp

defaults
	tls on
	logfile /dev/null

account aliMail
	host smtp.mxhichina.com
	from me@memory-yancy.com
	auth on
	user memory-yancy

account gmail
	host smtp.gmail.com
	from memory.yancy@gmail.com
	auth on
	user memory.yancy

account Foxmail
	host smtp.qq.com
	from memory_yancy@foxmail.com
	auth on
	user memory-yancy

account default : aliMail
